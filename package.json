{
  "name": "image-compressor",
  "version": "0.0.1",
  "description": "Compresses, resizes, and renames images.",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run clean && webpack && cross-env DEVELOPMENT=true electron dist/main.js",
    "clean": "del-cli ./dist",
    "build": "npm run clean ./build && webpack && electron-builder -p never",
    "release": "npm run clean ./build && webpack && electron-builder -p always"
  },
  "keywords": [
    "image",
    "compressor",
    "resizer",
    "renamer",
    "anime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tenpi/Image-Compressor"
  },
  "build": {
    "appId": "com.tenpi.image-compressor",
    "productName": "Image Compressor",
    "icon": "./assets/icon.png",
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "uninstallDisplayName": "Image Compressor",
      "createDesktopShortcut": "always",
      "installerSidebar": "./assets/installer.bmp",
      "uninstallerSidebar": "./assets/installer.bmp"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github",
      "repo": "Image-Compressor",
      "owner": "Tenpi",
      "releaseType": "draft"
    }
  },
  "author": "Tenpi",
  "license": "MIT",
  "devDependencies": {
    "@tenpi/tslint": "^1.1.0",
    "@types/gif-encoder": "^0.7.1",
    "@types/imagemin": "^8.0.0",
    "@types/imagemin-gifsicle": "^7.0.1",
    "@types/imagemin-mozjpeg": "^8.0.1",
    "@types/imagemin-optipng": "^5.2.1",
    "@types/imagemin-webp": "^5.1.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sharp": "^0.29.3",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "del-cli": "^3.0.1",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "less": "^4.1.0",
    "less-loader": "^7.3.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-loader": "^1.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "wasm-loader": "^1.3.0",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "bing-translate-api": "^2.2.1",
    "bootstrap": "^4.6.0",
    "electron-shortcuts": "^0.4.0",
    "electron-store": "^7.0.1",
    "electron-updater": "^4.3.5",
    "gif-encoder": "^0.7.2",
    "gif-frames": "github:benwiley4000/gif-frames",
    "image-pixels": "^2.2.2",
    "image-size": "^0.9.3",
    "imagemin": "github:tenpi/imagemin#fix2",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngcrush": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^6.0.0",
    "rc-slider": "^9.7.4",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.4.2",
    "react-reorder": "^3.0.0-alpha.7",
    "sagiri": "^3.1.1",
    "shade-blend-color": "^1.0.0",
    "sharp": "^0.29.2",
    "sharp-phash": "^2.0.0"
  }
}
